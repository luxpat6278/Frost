{
    "compilerOptions": {
      "target": "es2016",
      "lib": ["dom", "esnext"],
      "jsx": "react-jsx",
      "module": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "noEmit": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "allowJs": true,
      "checkJs": false 
    },
    "include": ["src"]
  }